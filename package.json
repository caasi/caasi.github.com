{
  "name": "caasih.net",
  "version": "0.1.0",
  "description": "my own graveyard",
  "main": "src/index.js",
  "scripts": {
    "start": "npm run clean && cross-env NODE_ENV=development webpack-dev-server",
    "clean": "rimraf ./dist/*.bundle.js ./dist/*.vendor.js ./dist/index.html ./dist/404.html",
    "clean:manifest": "rimraf ./dist/*.manifest.js",
    "lint": "eslint src",
    "build:will": "pug --out ./dist ./src/continuity.jade",
    "build:webpack": "webpack --config ./build.webpack.config.js",
    "build:snapshot": "react-snapshot --build-dir ./dist",
    "build": "npm run clean && cross-env NODE_ENV=production npm run build:webpack && npm run build:snapshot && npm run clean:manifest",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "test": "jest",
    "codecov": "jest && codecov",
    "flow": "flow",
    "prettier": "npx prettier --single-quote --no-semi --trailing-comma es5 --arrow-parens always --write *.js \"{config,flow,flow-typed}/**/*.js\""
  },
  "keywords": [
    "react",
    "personal"
  ],
  "author": "caasi Huang",
  "license": "MIT",
